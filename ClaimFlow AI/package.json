{
  "name": "claimflow-ai",
  "version": "1.0.0",
  "description": "HIPAA-compliant prior authorization management system with AI-powered document processing",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "frontend:dev": "vite --config vite.config.js",
    "frontend:build": "vite build --config vite.config.js",
    "frontend:preview": "vite preview --config vite.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node src/database/migrate.js",
    "seed": "node src/database/seed.js",
    "lint": "eslint src/ tests/",
    "lint:fix": "eslint src/ tests/ --fix",
    "security:audit": "npm audit",
    "security:check": "npm audit --audit-level moderate"
  },
  "keywords": [
    "healthcare",
    "prior-authorization",
    "hipaa",
    "medical",
    "ai",
    "document-processing",
    "compliance"
  ],
  "author": "ClaimFlow AI Team",
  "license": "PROPRIETARY",
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.888.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/x-data-grid": "^8.11.2",
    "@supabase/supabase-js": "^2.39.0",
    "axios": "^1.12.2",
    "bcrypt": "^5.1.1",
    "compression": "^1.7.4",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "natural": "^6.5.0",
    "node-cron": "^3.0.3",
    "node-forge": "^1.3.1",
    "nodemailer": "^6.9.3",
    "otplib": "^12.0.1",
    "pdf-parse": "^1.1.1",
    "pg": "^8.11.3",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.9.1",
    "recharts": "^3.2.0",
    "redis": "^4.6.12",
    "sharp": "^0.32.1",
    "speakeasy": "^2.0.0",
    "sqlite3": "^5.1.7",
    "tesseract.js": "^4.1.1",
    "twilio": "^5.9.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.7.0",
    "babel-jest": "^29.7.0",
    "eslint": "^8.55.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "jest-sonar-reporter": "^2.0.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "vite": "^6.3.6"
  },
  "jest": {
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/database/seed.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    }
  },
  "eslintConfig": {
    "extends": "standard",
    "env": {
      "node": true,
      "jest": true
    },
    "rules": {
      "no-console": "warn",
      "prefer-const": "error",
      "no-var": "error"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/claimflow-ai/backend.git"
  },
  "bugs": {
    "url": "https://github.com/claimflow-ai/backend/issues"
  },
  "homepage": "https://github.com/claimflow-ai/backend#readme"
}
