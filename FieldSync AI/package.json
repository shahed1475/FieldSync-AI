{
  "name": "insightflow-ai-backend",
  "version": "1.0.0",
  "description": "Backend system for InsightFlow AI - an analytics platform that converts natural language into business insights",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "docker:build": "docker build -t fieldsync-backend .",
    "docker:build:dev": "docker build -f Dockerfile.dev -t fieldsync-backend:dev .",
    "docker:up": "docker-compose up -d",
    "docker:up:dev": "docker-compose -f docker-compose.yml -f docker-compose.override.yml up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v --rmi all --remove-orphans",
    "ssl:setup": "node scripts/setup-ssl.js",
    "deploy:production": "node scripts/deploy-production.js",
    "deploy:staging": "NODE_ENV=staging node scripts/deploy-production.js",
    "health:check": "curl -f http://localhost:3000/health || exit 1",
    "health:ssl": "curl -f https://localhost:3000/health/ssl || exit 1"
  },
  "keywords": [
    "analytics",
    "ai",
    "insights",
    "postgresql",
    "express"
  ],
  "author": "InsightFlow AI Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.1",
    "compromise": "^14.10.0",
    "cors": "^2.8.5",
    "csv-parser": "^3.2.0",
    "csv-stringify": "^6.6.0",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "googleapis": "^128.0.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "ml-regression": "^6.0.1",
    "moment": "^2.30.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "mysql2": "^3.15.1",
    "natural": "^6.12.0",
    "node-cron": "^4.2.1",
    "node-quickbooks": "^2.0.34",
    "node-sql-parser": "^4.17.0",
    "openai": "^4.20.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-oauth2": "^1.8.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.7",
    "shopify-api-node": "^3.12.6",
    "simple-statistics": "^7.8.3",
    "sql-formatter": "^15.3.2",
    "stripe": "^14.9.0",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}